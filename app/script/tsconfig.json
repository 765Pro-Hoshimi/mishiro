{
  "compilerOptions": {
    "strict": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es6",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true
  },
  "include": [
    "../src/ts/@types/**/*",
    "./**/*",
    "../package.json"
  ]
}
