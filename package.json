{
  "name": "mishiro",
  "productName": "mishiro",
  "companyName": "Toyo",
  "version": "1.1.3",
  "description": "A desktop application that can be used for getting the resource of CGSS (IDOL MASTER CINDERELLA GIRLS STARLIGHT STAGE).",
  "main": "main.min.js",
  "scripts": {
    "start": "electron . --enable-logging",
    "webpack": "webpack",
    "dll": "webpack --config webpack.dll.config.js",
    "dev": "webpack --config webpack.dll.config.js&&webpack -w",
    "prod": "set NODE_ENV=production&&webpack --config webpack.dll.config.js&&webpack",
    "pkg32": "npm run prod&&electron-packager . %npm_package_name% --platform=win32 --arch=ia32 --icon=./src/res/icon/%npm_package_name%.ico --out=./dist --overwrite --app-version=%npm_package_version% --win32metadata.FileDescription=\"CGSS Desktop Application\" --ignore=\"(node_modules|data|download|dist|src|docs|public/img/card|public/asset/sound/live|.gitignore|README.md|webpack|.eslintrc.json|config.json|manifest.json)\"",
    "pkg64": "npm run prod&&electron-packager . %npm_package_name% --platform=win32 --arch=x64 --icon=./src/res/icon/%npm_package_name%.ico --out=./dist --overwrite --app-version=%npm_package_version% --win32metadata.FileDescription=\"CGSS Desktop Application\" --ignore=\"(node_modules|data|download|dist|src|docs|public/img/card|public/asset/sound/live|.gitignore|README.md|webpack|.eslintrc.json|config.json|manifest.json)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toyobayashi/mishiro"
  },
  "keywords": [
    "CGSS",
    "idol",
    "master",
    "346",
    "mishiro"
  ],
  "author": {
    "name": "Toyobayashi",
    "url": "https://github.com/toyobayashi"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^0.28.7",
    "electron": "^1.7.10",
    "electron-packager": "^9.1.0",
    "eslint": "^4.7.2",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "request": "^2.83.0",
    "sql.js": "^0.4.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "url-loader": "^0.5.9",
    "vue": "^2.5.13",
    "vue-i18n": "^7.3.3",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.6.0"
  },
  "dependencies": {}
}
