{
  "name": "mishiro",
  "productName": "mishiro",
  "companyName": "Toyo",
  "version": "1.3.3",
  "description": "mishiro",
  "main": "./public/mishiro.main.js",
  "scripts": {
    "start": "electron . --enable-logging",
    "webpack": "set TS_NODE_PROJECT=./build/tsconfig.json&&webpack --config ./build/webpack.config.ts",
    "dll": "set TS_NODE_PROJECT=./build/tsconfig.json&&webpack --config ./build/webpack.dll.config.ts",
    "dev": "set TS_NODE_PROJECT=./build/tsconfig.json&&webpack --config ./build/webpack.config.ts -w",
    "prod": "set NODE_ENV=production&&npm run dll&&npm run webpack",
    "pkg32": "npm run reb:hca-ia32&&npm run prod&&ts-node -P ./build/tsconfig.json ./build/packager.ts ia32",
    "pkg64": "npm run reb:hca-x64&&npm run prod&&ts-node -P ./build/tsconfig.json ./build/packager.ts x64",
    "count": "ts-node -P ./build/tsconfig.json ./build/source-count.ts",
    "dlc": "ts-node -P ./build/tsconfig.json ./build/release-download-count.ts",
    "reb:hca-ia32": "cd ./src/cpp/hca&&node-gyp rebuild --target=%npm_package_devDependencies_electron% --arch=ia32 --dist-url=https://atom.io/download/electron",
    "reb:hca-x64": "cd ./src/cpp/hca&&node-gyp rebuild --target=%npm_package_devDependencies_electron% --arch=x64 --dist-url=https://atom.io/download/electron",
    "sql32": "npm install sqlite3 --no-save --build-from-source --runtime=electron --target=%npm_package_devDependencies_electron% --target_arch=ia32 --dist-url=https://atom.io/download/electron",
    "sql64": "npm install sqlite3 --no-save --build-from-source --runtime=electron --target=%npm_package_devDependencies_electron% --target_arch=x64 --dist-url=https://atom.io/download/electron"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toyobayashi/mishiro"
  },
  "keywords": [
    "CGSS",
    "idol",
    "master",
    "346",
    "mishiro"
  ],
  "author": {
    "name": "Toyobayashi",
    "url": "https://github.com/toyobayashi"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/electron-packager": "^10.1.0",
    "@types/fs-extra": "^5.0.2",
    "@types/marked": "^0.3.0",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/msgpack-lite": "^0.1.6",
    "@types/node": "^9.6.4",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.3",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.1.3",
    "css-loader": "^0.28.11",
    "electron": "2.0.0",
    "electron-packager": "^12.0.2",
    "fs-extra": "^6.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "msgpack-lite": "^0.1.26",
    "native-addon-loader": "^1.0.0",
    "node-gyp": "^3.6.2",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "rijndael-js": "^1.0.0",
    "sqlite3": "^4.0.0",
    "ts-loader": "^4.2.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.2.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.0.14"
  },
  "dependencies": {
    "marked": "^0.3.14",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-i18n": "^7.4.2",
    "vue-property-decorator": "^6.0.0"
  }
}
